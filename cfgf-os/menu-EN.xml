				<action name="Execute">
					<execute>gksu ./.config/mobox-sz/iptStart</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/firewall.svg" label="Off">
				<action name="Execute">
					<execute>gksu ./.config/mobox-sz/iptStop</execute>
				</action>
			</item>
		</menu>
		<menu icon="//usr/share/icons/hicolor/48x48/apps/compton.png" id="5" label="Desktop effects">
			<item icon="//usr/share/icons/hicolor/48x48/apps/compton.png" label="On">
				<action name="Execute">
					<execute>./.config/mobox-sz/cmpEffOn</execute>
				</action>
			</item>
			<item icon="//usr/share/icons/hicolor/48x48/apps/compton.png" label="Off">
				<action name="Execute">
					<execute>./.config/mobox-sz/cmpEffOff</execute>
				</action>
			</item>
		</menu>
