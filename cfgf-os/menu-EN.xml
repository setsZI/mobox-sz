		</item>
		<item icon="/usr/share/icons/Menda-Circle/actions/scalable/system-reboot.svg" label="Reboot">
			<action name="Execute">
				<command>systemctl reboot</command>
			</action>
		</item>
		<item icon="/usr/share/icons/Menda-Circle/actions/scalable/system-shutdown.svg" label="Power off">
			<action name="Execute">
				<command>systemctl poweroff</command>
			</action>
		</item>
