					<execute>gksu manjaro-settings-manager</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/gparted.svg" label="Edit partitions">
				<action name="Execute">
					<execute>gksu gparted</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/preferences-desktop-display.svg" label="Screen">
				<action name="Execute">
					<execute>lxrandr</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/nvidia-settings.svg" label="NVIDIA props">
				<action name="Execute">
					<execute>nvidia-settings</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/wine-winecfg.svg" label="Wine settings">
				<action name="Execute">
					<execute>winecfg</execute>
				</action>
			</item>
		</menu>
		<menu icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/firewall.svg" id="4" label="Firewall">
