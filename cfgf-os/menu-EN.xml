				<execute>nitrogen</execute>
			</action>
		</item>
		<menu icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/tint2conf.svg" id="1" label="Panel">
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/desktop-environment-lxde.svg" label="LXDE panel">
				<action name="Execute">
					<execute>./.config/mobox-sz/lxOn</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/hicolor/48x48/apps/xfce4-panel-menu.png" label="XFCE panel">
				<action name="Execute">
					<execute>./.config/mobox-sz/xfOn</execute>
				</action>
			</item>
			<separator/>
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/tint2conf.svg" label="Panel TINT2">
				<action name="Execute">
					<execute>./.config/mobox-sz/tiOn</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/tint2conf.svg" label="TINT2 settings">
				<action name="Execute">
					<execute>tint2conf</execute>
				</action>
			</item>
		</menu>
		<menu icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/menu-editor.svg" id="2" label="Menu">
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/menu-editor.svg" label="Refresh">
				<action name="reconfigure"/>
			</item>
			<item icon="/usr/share/icons/Menda-Circle/apps/48x48/apps/menu-editor.svg" label="Edit">
				<action name="Execute">
					<execute>obmenu</execute>
				</action>
			</item>
			<separator/>
			<item icon="/usr/share/icons/Menda-Circle/categories/48x48/xfce-utils.svg" label="Config file">
				<action name="Execute">
					<execute>geany ./.config/openbox/menu.xml</execute>
				</action>
			</item>
		</menu>
		<menu icon="/usr/share/icons/Menda-Circle/categories/48x48/system_section.svg" id="3" label="OS settings">
			<item icon="/usr/share/icons/hicolor/scalable/apps/manjaro-settings-manager.svg" label="Base settings">
